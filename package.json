{
    "name": "altv-vscode-docs",
    "displayName": "alt:V IDE",
    "description": "alt:V IDE. It is an Intellisense assistant for alt:V. Fixes common setup mistakes, hovering over functions gives examples, installs types, and much more...",
    "version": "1.0.5",
    "publisher": "stuyk",
    "repository": "https://github.com/stuyk/altv-vscode-docs",
    "engines": {
        "vscode": "^1.34.0"
    },
    "license": "MIT",
    "categories": [
        "Other",
        "Snippets",
        "Visualization"
    ],
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#1F1F1F",
        "theme": "dark"
    },
    "activationEvents": [
        "workspaceContains:altv-server.*"
    ],
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint . --ext .ts,.tsx",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^12.12.0",
        "@types/vscode": "^1.34.0",
        "@typescript-eslint/eslint-plugin": "^3.0.2",
        "@typescript-eslint/parser": "^3.0.2",
        "eslint": "^7.1.0",
        "typescript": "^4.0.2"
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 120,
        "semi": true,
        "singleQuote": true
    }
}
