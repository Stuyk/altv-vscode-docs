{
    "name": "altv-vscode-docs",
    "displayName": "alt:V IDE",
    "description": "alt:V IDE. It is an Intellisense assistant for alt:V. Documentation browser. Fixes common setup mistakes, hovering over functions gives examples, installs types, and much more...",
    "version": "2.0.8",
    "publisher": "stuyk",
    "repository": "https://github.com/stuyk/altv-vscode-docs",
    "engines": {
        "vscode": "^1.53.0"
    },
    "license": "MIT",
    "categories": [
        "Other",
        "Snippets",
        "Visualization"
    ],
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#1F1F1F",
        "theme": "dark"
    },
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "git submodule update --init --recursive && tsc -p ./",
        "lint": "eslint . --ext .ts,.tsx",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/node": "^12.12.0",
        "@types/unzipper": "^0.10.4",
        "@types/vscode": "^1.53.0",
        "@typescript-eslint/eslint-plugin": "^3.0.2",
        "@typescript-eslint/parser": "^3.0.2",
        "eslint": "^7.1.0",
        "typescript": "^4.0.2"
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 120,
        "semi": true,
        "singleQuote": true
    },
    "dependencies": {
        "download-git-repo": "^3.0.2",
        "glob": "^7.1.6",
        "gray-matter": "^4.0.2",
        "unzipper": "^0.10.11",
        "wget-improved": "^3.2.1"
    },
    "contributes": {
        "commands": [
            {
                "command": "altv-docs",
                "title": "alt:V Documentation"
            },
            {
                "command": "altv-vscode-docs.altv-docs-refresh",
                "title": "alt:V Documentation (Refresh)"
            }
        ]
    }
}
